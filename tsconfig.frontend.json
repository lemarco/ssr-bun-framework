{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"outDir": "./dist",
		"removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"inlineSources": true,
		"inlineSourceMap": true,
		"jsx": "react-jsx"
	},
	"include": ["pages"]
}
